{
  "name": "@-amazeelabs/silverback-gastby",
  "private": true,
  "description": "gatsby-graphql-toolkit example for drupal",
  "version": "1.2.115",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.13.51",
    "@amazeelabs/gatsby-source-silverback": "^1.6.11",
    "@amazeelabs/gatsby-theme-core": "^0.6.52",
    "@amazeelabs/silverback-iframe": "^1.1.39",
    "axios": "0.27.2",
    "dotenv": "16.0.0",
    "gatsby": "4.13.1",
    "gatsby-graphql-source-toolkit": "2.0.2",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-remote-images": "3.4.0",
    "gatsby-plugin-schema-export": "^1.1.21",
    "gatsby-plugin-sharp": "4.13.0",
    "gatsby-transformer-sharp": "4.13.0",
    "html-react-parser": "1.4.12",
    "netlify": "11.0.1",
    "p-queue": "6.6.2",
    "postcss-import": "14.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.25.1",
    "strip-ansi": "6.0.1",
    "tailwindcss": "3.0.24",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.27",
    "@amazeelabs/jest-preset": "^1.3.30",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@formatjs/cli": "4.8.4",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.9",
    "@graphql-codegen/typescript-operations": "2.3.6",
    "@types/classnames": "2.3.1",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "babel-plugin-formatjs": "10.3.20",
    "babel-preset-gatsby": "2.13.0",
    "eslint": "8.14.0",
    "gatsby-source-filesystem": "4.13.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "yarn codegen && yarn translations:extract",
    "build": "yarn translations:push_sources && gatsby build",
    "develop": "yarn translations:push_sources && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "fast-builds:serve:local": "ts-node fast-builds/serve.ts 9001 local",
    "fast-builds:run:local": "ts-node fast-builds/local.ts",
    "fast-builds:serve:netlify": "ts-node fast-builds/serve.ts 3000 netlify",
    "fast-builds:run:netlify": "ts-node fast-builds/netlify.ts",
    "update-schema": "SCHEMA_UPDATE=true gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file=generated/translation_sources.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:push_sources": "ts-node scripts/push_translation_sources.ts generated/translation_sources.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  }
}
