{
  "name": "@-amazeelabs/silverback-gastby",
  "private": true,
  "description": "gatsby-graphql-toolkit example for drupal",
  "version": "2.0.26",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "workspace:*",
    "@amazeelabs/gatsby-silverback-cloudinary": "workspace:*",
    "@amazeelabs/gatsby-source-silverback": "workspace:*",
    "@amazeelabs/gatsby-theme-core": "workspace:*",
    "@amazeelabs/silverback-graphql-persisted": "workspace:*",
    "@amazeelabs/silverback-iframe": "workspace:*",
    "axios": "1.3.4",
    "domhandler": "5.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "gatsby": "5.8.0",
    "gatsby-graphql-source-toolkit": "2.0.4",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-pnpm": "1.2.10",
    "gatsby-plugin-remote-images": "3.6.5",
    "gatsby-plugin-schema-export": "workspace:*",
    "gatsby-plugin-sharp": "5.8.0",
    "gatsby-transformer-sharp": "5.8.0",
    "html-react-parser": "3.0.13",
    "netlify": "13.1.3",
    "p-queue": "6.6.2",
    "postcss-import": "15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.3.2",
    "react-query": "3.39.3",
    "strip-ansi": "6.0.1",
    "tailwindcss": "3.2.7",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@amazeelabs/codegen-operation-ids": "workspace:*",
    "@amazeelabs/eslint-config": "workspace:*",
    "@amazeelabs/jest-preset": "workspace:*",
    "@amazeelabs/prettier-config": "workspace:*",
    "@formatjs/cli": "6.0.4",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-operations": "3.0.2",
    "@types/classnames": "2.3.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "16.18.18",
    "@types/react": "18.0.28",
    "@types/tmp": "0.2.3",
    "babel-plugin-formatjs": "10.4.0",
    "babel-preset-gatsby": "3.8.0",
    "eslint": "8.36.0",
    "gatsby-source-filesystem": "5.8.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.6",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pnpm codegen && pnpm translations:extract",
    "build:gatsby": "pnpm translations:push_sources && gatsby build",
    "develop": "pnpm translations:push_sources && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "fast-builds:serve:local": "ts-node fast-builds/serve.ts 9001 local",
    "fast-builds:run:local": "ts-node fast-builds/local.ts",
    "fast-builds:serve:netlify": "ts-node fast-builds/serve.ts 3000 netlify",
    "fast-builds:run:netlify": "ts-node fast-builds/netlify.ts",
    "update-schema": "SCHEMA_UPDATE=true gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file=generated/translation_sources.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:push_sources": "ts-node scripts/push_translation_sources.ts generated/translation_sources.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  }
}
