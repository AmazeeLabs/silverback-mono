{
  "name": "@-amazeelabs/silverback-gastby",
  "private": true,
  "description": "gatsby-graphql-toolkit example for drupal",
  "version": "1.2.101",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.13.38",
    "@amazeelabs/gatsby-source-silverback": "^1.6.10",
    "@amazeelabs/gatsby-theme-core": "^0.6.51",
    "@amazeelabs/silverback-iframe": "^1.1.38",
    "axios": "0.26.1",
    "dotenv": "16.0.0",
    "gatsby": "4.12.1",
    "gatsby-graphql-source-toolkit": "2.0.2",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-remote-images": "3.3.0",
    "gatsby-plugin-schema-export": "^1.1.20",
    "gatsby-plugin-sharp": "4.12.1",
    "gatsby-transformer-sharp": "4.12.1",
    "html-react-parser": "1.4.11",
    "netlify": "11.0.1",
    "p-queue": "6.6.2",
    "postcss-import": "14.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.24.6",
    "strip-ansi": "6.0.1",
    "tailwindcss": "3.0.24",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.26",
    "@amazeelabs/jest-preset": "^1.3.29",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@formatjs/cli": "^4.8.2",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@types/classnames": "2.3.1",
    "@types/jest": "27.4.1",
    "babel-plugin-formatjs": "^10.3.18",
    "babel-preset-gatsby": "^2.10.0",
    "@types/node": "16.11.27",
    "eslint": "8.13.0",
    "gatsby-source-filesystem": "4.12.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.0",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "yarn codegen",
    "build": "yarn translations:push_sources && gatsby build",
    "develop": "yarn translations:push_sources && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "fast-builds:serve:local": "ts-node fast-builds/serve.ts 9001 local",
    "fast-builds:run:local": "ts-node fast-builds/local.ts",
    "fast-builds:serve:netlify": "ts-node fast-builds/serve.ts 3000 netlify",
    "fast-builds:run:netlify": "ts-node fast-builds/netlify.ts",
    "update-schema": "SCHEMA_UPDATE=true gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file=.cache/translation_sources.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:push_sources" : "yarn translations:extract && ts-node scripts/push_translation_sources.ts .cache/translation_sources.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  }
}
