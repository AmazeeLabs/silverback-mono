{
  "name": "@-amazeelabs/silverback-gatsby",
  "private": true,
  "description": "gatsby-graphql-toolkit example for drupal",
  "version": "3.0.15",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "workspace:*",
    "@amazeelabs/gatsby-silverback-cloudinary": "workspace:*",
    "@amazeelabs/gatsby-source-silverback": "workspace:*",
    "@amazeelabs/graphql-directives": "workspace:*",
    "@amazeelabs/publisher": "workspace:*",
    "@amazeelabs/silverback-iframe": "workspace:*",
    "@amazeelabs/test-directives": "workspace:*",
    "axios": "1.5.1",
    "domhandler": "5.0.3",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "gatsby": "5.12.7",
    "gatsby-graphql-source-toolkit": "2.0.4",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-pnpm": "1.2.10",
    "gatsby-plugin-remote-images": "3.6.6",
    "gatsby-plugin-schema-export": "workspace:*",
    "gatsby-plugin-sharp": "5.12.1",
    "gatsby-transformer-sharp": "5.12.1",
    "html-react-parser": "4.2.6",
    "netlify-cli": "16.8.0",
    "postcss-import": "15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.5.0",
    "react-query": "3.39.3",
    "tailwindcss": "3.3.3",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@amazeelabs/codegen-operation-ids": "workspace:*",
    "@amazeelabs/eslint-config": "1.4.43",
    "@amazeelabs/prettier-config": "1.1.3",
    "@formatjs/cli": "6.2.1",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@types/classnames": "2.3.1",
    "@types/express": "4.17.20",
    "@types/node": "16.18.59",
    "@types/react": "18.2.29",
    "@types/tmp": "0.2.5",
    "babel-plugin-formatjs": "10.5.7",
    "babel-preset-gatsby": "3.12.0",
    "eslint": "8.51.0",
    "gatsby-source-filesystem": "5.12.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pnpm codegen && pnpm translations:extract && source .envrc",
    "build:gatsby": "pnpm translations:push_sources && gatsby build",
    "develop": "pnpm translations:push_sources && gatsby develop",
    "start": "publisher",
    "open": "open http://127.0.0.1:8000/___status/",
    "clean": "gatsby clean",
    "update-schema": "SCHEMA_UPDATE=true gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file=generated/translation_sources.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:push_sources": "ts-node scripts/push_translation_sources.ts generated/translation_sources.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  }
}
