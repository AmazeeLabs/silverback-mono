{
  "name": "@-amazeelabs/silverback-gastby",
  "private": true,
  "description": "gatsby-graphql-toolkit example for drupal",
  "version": "1.2.170",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.15.9",
    "@amazeelabs/gatsby-source-silverback": "^1.9.8",
    "@amazeelabs/gatsby-theme-core": "^0.6.64",
    "@amazeelabs/silverback-graphql-persisted": "^1.1.11",
    "@amazeelabs/silverback-iframe": "^1.1.50",
    "axios": "0.27.2",
    "dotenv": "16.0.3",
    "gatsby": "4.24.6",
    "gatsby-graphql-source-toolkit": "2.0.2",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-remote-images": "3.5.0",
    "gatsby-plugin-schema-export": "^1.1.29",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-transformer-sharp": "4.24.0",
    "html-react-parser": "3.0.4",
    "netlify": "12.0.3",
    "p-queue": "6.6.2",
    "postcss-import": "15.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.2.1",
    "react-query": "3.39.2",
    "strip-ansi": "6.0.1",
    "tailwindcss": "3.2.1",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@amazeelabs/codegen-operation-ids": "^0.1.4",
    "@amazeelabs/eslint-config": "^1.4.36",
    "@amazeelabs/jest-preset": "^1.3.40",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@formatjs/cli": "5.1.3",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@types/classnames": "2.3.1",
    "@types/jest": "29.2.1",
    "@types/node": "16.18.3",
    "babel-plugin-formatjs": "10.3.31",
    "babel-preset-gatsby": "2.24.0",
    "eslint": "8.26.0",
    "gatsby-source-filesystem": "4.24.0",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "yarn codegen && yarn translations:extract",
    "build": "yarn translations:push_sources && gatsby build",
    "develop": "yarn translations:push_sources && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "fast-builds:serve:local": "ts-node fast-builds/serve.ts 9001 local",
    "fast-builds:run:local": "ts-node fast-builds/local.ts",
    "fast-builds:serve:netlify": "ts-node fast-builds/serve.ts 3000 netlify",
    "fast-builds:run:netlify": "ts-node fast-builds/netlify.ts",
    "update-schema": "SCHEMA_UPDATE=true gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file=generated/translation_sources.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:push_sources": "ts-node scripts/push_translation_sources.ts generated/translation_sources.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  }
}
