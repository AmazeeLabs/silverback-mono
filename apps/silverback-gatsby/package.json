    "p-queue": "^6.6.2",
    "postcss-import": "^14.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "strip-ansi": "^6.0.0",
    "tailwindcss": "^2.0.3",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.6",
    "@amazeelabs/jest-preset": "^1.3.11",
