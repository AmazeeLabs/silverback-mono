type Change {
  type: ChangeType!
  id: String!
}

enum ChangeType {
  Update
  Delete
}

interface Translatable {
  id: String!
  translations: [Translation!]!
}

interface Translation {
  langcode: String!
}

type Feed {
  typeName: String!
  translationsTypeName: String
  singleFieldName: String!
  listFieldName: String!
  changesFieldName: String
}
