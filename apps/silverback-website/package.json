{
  "name": "silverback-website",
  "description": "Silverback website",
  "version": "0.8.0",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@amazeelabs/gatsby-theme-core": "^0.3.0",
    "@mdx-js/mdx": "^1.6.18",
    "@mdx-js/react": "^1.6.18",
    "@tailwindcss/typography": "^0.2.0",
    "@tailwindcss/ui": "^0.6.2",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.69",
    "gatsby-plugin-mdx": "^1.2.46",
    "gatsby-plugin-sharp": "^2.6.39",
    "gatsby-remark-autolink-headers": "^2.3.15",
    "gatsby-remark-images": "^3.3.33",
    "gatsby-remark-relative-links": "^0.0.3",
    "gatsby-source-filesystem": "^2.3.34",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config-react": "^1.0.7",
    "@amazeelabs/prettier-config": "^1.1.0",
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@types/classnames": "^2.2.10",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.11.5",
    "eslint": "^7.9.0",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "gatsby-plugin-schema-export": "^1.1.0",
    "gatsby-plugin-schema-snapshot": "^1.0.0",
    "prettier": "2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "update-schema": "GATSBY_UPDATE_SCHEMA_SNAPSHOT=true gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "pretest": "yarn lint && yarn format",
    "typecheck": "yarn codegen && tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "yarn typecheck"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://localhost:4873",
    "repository": "git@github.com:AmazeeLabs/silverback-website.git",
    "branch": "master"
  }
}
