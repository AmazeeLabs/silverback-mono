{
  "compilerOptions": {
    "noEmit": false,
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": false,
    "outDir": "./dist",
    "declaration": true,
    "declarationDir": "types",
    "types": ["cypress"],
    "typeRoots": ["types"]
  },
  "include": ["src", "index.ts"],
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
