{
  "name": "cypress-screenplay",
  "version": "0.5.64",
  "description": "Screenplay pattern implementation for cypress.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "author": "Philipp Melab",
  "license": "MIT",
  "scripts": {
    "lint": "eslint '*/**.ts'",
    "formatting": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "formatting:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "tsc",
    "cypress": "cypress open",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && pnpm lint",
    "test:unit": "mocha -r ts-node/register src/__tests__/*.spec.ts",
    "test:integration": "cypress run",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.42",
    "@amazeelabs/jest-preset": "1.3.46",
    "@amazeelabs/prettier-config": "1.1.2",
    "@tsconfig/recommended": "1.0.2",
    "@types/jest": "29.5.0",
    "@types/mocha": "10.0.1",
    "@types/sinon": "10.0.13",
    "chai": "4.3.7",
    "cypress": "9.7.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "eslint": "8.36.0",
    "jest": "29.5.0",
    "mocha": "10.2.0",
    "mocha-typescript": "1.1.17",
    "prettier": "2.8.4",
    "semantic-release": "20.1.1",
    "sinon": "15.0.2",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
