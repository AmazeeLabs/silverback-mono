    "directory": "packages/npm/drupal-test-session-cypress"
  },
  "author": "Amazee Labs",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "tsc --outDir . --declaration",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.15",
    "@amazeelabs/jest-preset": "^1.3.18",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.4.0",
    "cypress": "9.4.1",
    "eslint": "8.8.0",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  }
}
