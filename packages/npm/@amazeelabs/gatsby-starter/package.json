{
  "name": "@amazeelabs/gatsby-starter",
  "private": true,
  "description": "Gatsby starter for Amazee Labs projects",
  "version": "0.5.0",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "@amazeelabs/gatsby-theme-core": "^0.3.0",
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@types/classnames": "^2.2.10",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.58",
    "gatsby-plugin-schema-export": "^1.1.0",
    "gatsby-plugin-schema-snapshot": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.28",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config-react": "^1.0.7",
    "@amazeelabs/prettier-config": "^1.1.0",
    "@types/node": "^14.10.1",
    "eslint": "^7.9.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "prettier": "2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "update-schema": "GATSBY_UPDATE_SCHEMA_SNAPSHOT=true gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "pretest": "yarn lint && yarn format",
    "typecheck": "yarn codegen && tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "yarn typecheck"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/silverback-mono/issues"
  }
}
