{
  "name": "@amazeelabs/molecules",
  "version": "0.4.4",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "prepare": "yarn build",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "storybook": "start-storybook -p 6007 --ci",
    "build-storybook": "build-storybook",
    "deploy": "yarn build-storybook && yarn netlify deploy -a $NETLIFY_AUTH_TOKEN -d storybook-static -p -s amazeelabs-molecules"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.35",
    "@amazeelabs/jest-preset": "^1.3.39",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@babel/core": "7.19.3",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/ui": "0.24.1",
    "babel-loader": "8.2.5",
    "eslint": "8.25.0",
    "eslint-plugin-promise": "6.1.0",
    "eslint-plugin-storybook": "0.6.6",
    "happy-dom": "6.0.4",
    "lodash": "4.17.21",
    "netlify-cli": "11.8.3",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "storybook": "7.0.0-alpha.37",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-imagetools": "4.0.11",
    "vite-plugin-dts": "1.6.5",
    "vitest": "0.24.1"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
