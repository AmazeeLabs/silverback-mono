    "react-dom": "18.3.1",
    "storybook": "8.0.10",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-imagetools": "7.0.2",
    "vite-plugin-dts": "3.9.1",
    "vitest": "1.6.0",
    "zustand": "4.5.2"
  },
