{
  "name": "@amazeelabs/molecules",
  "version": "0.3.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "prepare": "yarn build",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "vitest",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "deploy": "yarn build-storybook && yarn netlify deploy -a $NETLIFY_AUTH_TOKEN -d storybook-static -p -s amazeelabs-molecules"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.30",
    "@amazeelabs/jest-preset": "^1.3.34",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@babel/core": "^7.19.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.23.2",
    "babel-loader": "^8.2.5",
    "eslint": "8.22.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-storybook": "^0.6.4",
    "happy-dom": "^6.0.4",
    "lodash": "^4.17.21",
    "netlify-cli": "^11.8.0",
    "postcss": "^8.4.16",
    "prettier": "^2",
    "storybook": "^7.0.0-alpha.33",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4",
    "vite": "3.1.0",
    "vite-imagetools": "^4.0.9",
    "vite-plugin-dts": "^1.5.0",
    "vitest": "^0.23.2"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
