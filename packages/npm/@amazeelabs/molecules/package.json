{
  "name": "@amazeelabs/molecules",
  "version": "0.5.45",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run --passWithNoTests && pnpm build-storybook",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6007 --ci",
    "build-storybook": "storybook build",
    "deploy": "pnpm build-storybook && pnpm run --filter=@amazeelabs/molecules netlify deploy -a $NETLIFY_AUTH_TOKEN -d packages/npm/@amazeelabs/molecules/storybook-static -p -s amazeelabs-molecules"
  },
