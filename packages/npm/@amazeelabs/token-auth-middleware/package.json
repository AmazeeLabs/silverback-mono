{
  "name": "@amazeelabs/token-auth-middleware",
  "description": "Middleware for token based authentication",
  "version": "1.1.8",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
