{
  "name": "@amazeelabs/publisher-elements",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/elements.es.js",
  "exports": {
    ".": "./dist/elements.es.js"
  },
  "types": "types/elements.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "develop": "pnpm dev:ui",
    "dev:ui": "vite",
    "build:ui": "tsc && vite build",
    "build:tailwind:watch": "tailwind -w -i tailwind.css -o tailwind.build.css",
    "build:tailwind": "tailwind -i tailwind.css -o tailwind.build.css",
    "build:watch": "concurrently 'pnpm build:tailwind:watch' 'pnpm storybook'",
    "preview": "vite preview",
    "build": "pnpm build:tailwind && pnpm build:ui",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lit": "^2.6.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.40",
    "@amazeelabs/jest-preset": "1.3.44",
    "@amazeelabs/prettier-config": "1.1.1",
    "@amazeelabs/react-framework-bridge": "workspace:^2.3.15",
    "@babel/core": "7.20.12",
    "@headlessui/react": "1.7.8",
    "@react-rxjs/core": "0.10.3",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-json": "5.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.2",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-toolbars": "6.5.16",
    "@storybook/addon-viewport": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/web-components": "6.5.16",
    "@types/colors": "1.2.1",
    "@types/http-proxy-middleware": "1.0.0",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.191",
    "@types/lodash-es": "4.17.6",
    "@types/morgan": "1.9.4",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "9.1.2",
    "clsx": "1.2.1",
    "colors": "1.4.0",
    "concurrently": "7.6.0",
    "esbuild": "0.17.5",
    "eslint": "8.33.0",
    "eslint-plugin-storybook": "0.6.10",
    "jest": "29.4.1",
    "lit": "2.6.1",
    "lodash-es": "4.17.21",
    "prettier": "2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lazylog": "4.5.3",
    "react-rxjs": "3.0.0-alpha.6",
    "rollup": "3.12.1",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-lit-css": "4.0.1",
    "rxjs": "7.8.0",
