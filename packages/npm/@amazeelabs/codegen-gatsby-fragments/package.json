{
  "name": "@amazeelabs/codegen-gatsby-fragments",
  "version": "1.1.15",
  "description": "Generate fragments for Gatsby GraphQL queries.",
  "type": "commonjs",
  "main": "build/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc ./src -d ./build",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "graphql": "^16.8.1"
  },
  "optionalDependencies": {
    "@graphql-codegen/plugin-helpers": "^5.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "keywords": [],
  "author": "AmazeeLabs <development@amazeelabs.com>",
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.43",
    "@amazeelabs/prettier-config": "1.1.3",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.102",
    "eslint": "8.56.0",
    "prettier": "3.1.1",
    "typescript": "5.3.3"
  }
}
