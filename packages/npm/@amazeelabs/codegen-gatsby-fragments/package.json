{
  "name": "@amazeelabs/codegen-gatsby-fragments",
  "version": "1.1.22",
  "description": "Generate fragments for Gatsby GraphQL queries.",
  "type": "commonjs",
  "main": "build/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc ./src -d ./build",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "graphql": "^16.8.1"
  },
  "optionalDependencies": {
    "@graphql-codegen/plugin-helpers": "^5.0.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "keywords": [],
  "author": "AmazeeLabs <development@amazeelabs.com>",
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.43",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.5",
    "eslint": "8.57.0",
    "typescript": "5.4.5"
  }
}
