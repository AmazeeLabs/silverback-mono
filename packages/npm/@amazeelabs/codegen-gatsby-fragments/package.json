{
  "name": "@amazeelabs/codegen-gatsby-fragments",
  "version": "1.1.8",
  "description": "Generate fragments for Gatsby GraphQL queries.",
  "type": "commonjs",
  "main": "build/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc ./src -d ./build",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "graphql": "^16.6.0"
  },
  "optionalDependencies": {
    "@graphql-codegen/plugin-helpers": "^4.2.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  },
  "keywords": [],
  "author": "AmazeeLabs <development@amazeelabs.com>",
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.43",
    "@amazeelabs/prettier-config": "1.1.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.64",
    "eslint": "8.42.0",
    "prettier": "2.8.8",
    "typescript": "5.1.3"
  }
}
