{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "declaration": true,
    "declarationDir": "dts"
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
