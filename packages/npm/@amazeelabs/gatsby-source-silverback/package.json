{
  "name": "@amazeelabs/gatsby-source-silverback",
  "version": "1.6.4",
  "description": "Source data from a Silverback GraphQL schema.",
  "main": "index.js",
  "types": "types.d.ts",
  "author": "Amazee Labs <development@amazeelabs.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.21",
    "@amazeelabs/jest-preset": "^1.3.24",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.4.1",
    "eslint": "8.11.0",
    "gatsby": "4.10.0",
    "gatsby-graphql-source-toolkit": "2.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "prettier": "2.6.0",
    "react": "17.0.2",
    "typescript": "4.6.2"
  },
  "scripts": {
    "prepare": "tsc",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"src/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi",
    "test:watch": "jest --watch"
  }
}
