{
  "name": "@amazeelabs/react-intl-rsc",
  "version": "1.0.0",
  "description": "RSC-compatible wrapper for react-intl.",
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && swc ./src -d ./build && tsc --emitDeclarationOnly",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "prepare": "exit 0",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "react-intl": "^6.3.1"
  },
  "optionalDependencies": {
    "@types/react": "^18.0.28",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.23",
    "typescript": "4.9.5",
    "@amazeelabs/eslint-config": "^1.4.42",
    "eslint": "^8.36.0",
    "@amazeelabs/prettier-config": "^1.1.2",
    "prettier": "^2.8.4"
  },
  "author": "AmazeeLabs <development@amazeelabs.com>"
}
