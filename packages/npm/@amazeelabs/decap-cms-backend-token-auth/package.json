{
  "name": "@amazeelabs/decap-cms-backend-token-auth",
  "version": "1.1.11",
  "description": "",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./proxy": {
      "import": "./build/proxy.js"
    },
    "./backend": {
      "import": "./build/backend.js"
    }
  },
  "typesVersions": {
    "*": {
      "proxy": [
        "build/proxy.d.ts"
      ],
      "backend": [
        "build/backend.d.ts"
      ]
    }
  },
  "scripts": {
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\"",
    "test:unit": "vitest run --passWithNoTests",
    "prep": "rm -rf build && tsc -p tsconfig.build.json",
    "build": "pnpm prep",
    "watch": "tsc --watch -p tsconfig.build.json"
  },
  "dependencies": {
    "@amazeelabs/token-auth-middleware": "workspace:*",
    "@emotion/styled": "^11.11.5",
    "decap-cms-backend-github": "^3.1.2",
    "decap-cms-lib-util": "^3.0.4",
    "decap-cms-ui-default": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.43",
    "@octokit/types": "13.4.0",
    "@types/node": "18.19.31",
    "@types/react": "18.2.78",
    "@types/semaphore": "1.1.4",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
