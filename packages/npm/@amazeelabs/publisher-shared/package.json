{
  "name": "@amazeelabs/publisher-shared",
  "version": "2.0.5",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/exports.js",
  "types": "./dist/exports.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "pnpm build --watch",
