  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "workspace:^1.4.42",
    "@amazeelabs/jest-preset": "workspace:^1.3.46",
    "@amazeelabs/prettier-config": "workspace:^1.1.2",
    "rollup": "3.19.1",
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-esbuild": "5.0.0",
    "typescript": "4.9.5"
  }
}
