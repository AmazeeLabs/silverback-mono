{
  "name": "@amazeelabs/react-di",
  "version": "1.0.50",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.4",
    "@amazeelabs/jest-preset": "^1.3.18",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.4.0",
    "eslint": "8.8.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "scripts": {
    "prepare": "tsc",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@amazeelabs/eslint-config": "^1.4.15",
    "@amazeelabs/prettier-config": "^1.1.1"
  }
