{
  "name": "@amazeelabs/react-di",
  "version": "1.0.39",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.4",
    "@amazeelabs/jest-preset": "^1.3.12",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "^27",
    "eslint": "^8",
    "jest": "^27",
    "lint-staged": "^11.0.0",
    "prettier": "^2",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "prepare": "tsc",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@amazeelabs/eslint-config": "^1.4.6",
    "@amazeelabs/prettier-config": "^1.1.1"
  }
