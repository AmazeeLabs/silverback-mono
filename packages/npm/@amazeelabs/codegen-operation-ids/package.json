{
  "name": "@amazeelabs/codegen-operation-ids",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "test:watch": "vitest",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "yarn build && yarn graphql-codegen",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run --passWithNoTests",
    "test:integration": "exit 0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.7.1",
    "@graphql-codegen/visitor-plugin-common": "^2.12.1"
  },
  "peerDependencies": {
    "graphql": "> 14"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.33",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "eslint": "^8",
    "lint-staged": "13.0.3",
    "prettier": "^2",
    "typescript": "4.8.3",
    "vite": "3.1.8",
    "vitest": "0.24.1"
  }
}
