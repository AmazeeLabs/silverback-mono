{
  "name": "@amazeelabs/react-framework-bridge",
  "version": "2.3.10",
  "description": "Bridge code to implement framework independent react components.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./gatsby": {
      "import": "./dist/gatsby.js",
      "require": "./dist/gatsby.cjs"
    },
    "./storybook": {
      "import": "./dist/storybook.js",
      "require": "./dist/storybook.cjs"
    },
    "./react-intl": {
      "import": "./dist/react-intl.js",
      "require": "./dist/react-intl.cjs"
    },
    "./formik": {
      "import": "./dist/formik.js",
      "require": "./dist/formik.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "formik": [
        "./dist/formik.d.ts"
      ],
      "react-intl": [
        "./dist/react-intl.d.ts"
      ],
      "storybook": [
        "./dist/storybook.d.ts"
      ],
      "gatsby": [
        "./dist/gatsby.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "dependencies": {
    "formik": "2.2.9",
    "hast-util-has-property": "^2.0.0",
    "hast-util-is-element": "^2.1.2",
    "hast-util-select": "^5.0.3",
    "lodash": "^4.17.21",
    "qs": "^6.10.5",
    "react-error-boundary": "^3.1.4",
    "react-intl": "6.2.5",
    "rehype-parse": "^8.0.4",
    "rehype-react": "^7.1.1",
    "rehype-slug": "^5.1.0",
    "unified": "^10.1.2",
    "unist-util-modify-children": "^3.1.0",
    "unist-util-visit": "^4.1.1"
  },
  "peerDependencies": {
    "@storybook/addon-actions": ">=6.5.15",
    "gatsby": ">=5.3.3",
    "gatsby-plugin-image": ">=3.3.2",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "yup": ">=0.32.11"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.38",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@rollup/plugin-commonjs": "23.0.7",
    "@rollup/plugin-multi-entry": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "2.2.0",
    "esbuild": "0.16.14",
    "eslint": "8.31.0",
    "gatsby": "5.3.3",
    "gatsby-plugin-image": "3.3.2",
    "jsdom": "20.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "rollup": "3.9.1",
    "rollup-plugin-dts": "5.1.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-multi-input": "1.3.1",
    "typescript": "4.9.4",
    "vite": "3.2.5",
    "vite-plugin-dts": "1.7.1",
    "vitest": "0.26.3",
    "zustand": "4.2.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "yarn build:index && yarn build:gatsby && yarn build:storybook && yarn build:react-intl && yarn build:formik",
    "build:index": "ENTRYPOINT=index vite build",
    "build:gatsby": "ENTRYPOINT=gatsby vite build",
    "build:storybook": "ENTRYPOINT=storybook vite build",
    "build:formik": "ENTRYPOINT=formik vite build",
    "build:react-intl": "ENTRYPOINT=react-intl vite build",
    "dev": "vite build --watch",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run",
    "test:integration": "exit 0",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "access": "public"
  }
}
