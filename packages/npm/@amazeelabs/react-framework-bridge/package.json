{
  "name": "@amazeelabs/react-framework-bridge",
  "version": "2.3.19",
  "description": "Bridge code to implement framework independent react components.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./gatsby": {
      "import": "./dist/gatsby.js",
      "require": "./dist/gatsby.cjs"
    },
    "./storybook": {
      "import": "./dist/storybook.js",
      "require": "./dist/storybook.cjs"
    },
    "./react-intl": {
      "import": "./dist/react-intl.js",
      "require": "./dist/react-intl.cjs"
    },
    "./formik": {
      "import": "./dist/formik.js",
      "require": "./dist/formik.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "formik": [
        "./dist/formik.d.ts"
      ],
      "react-intl": [
        "./dist/react-intl.d.ts"
      ],
      "storybook": [
        "./dist/storybook.d.ts"
      ],
      "gatsby": [
        "./dist/gatsby.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "dependencies": {
    "formik": "2.2.9",
    "hast-util-has-property": "^2.0.1",
    "hast-util-is-element": "^2.1.3",
    "hast-util-select": "^5.0.5",
    "lodash": "^4.17.21",
    "qs": "^6.11.1",
    "react-error-boundary": "^3.1.4",
    "react-intl": "6.3.0",
    "rehype-parse": "^8.0.4",
    "rehype-react": "^7.1.2",
    "rehype-slug": "^5.1.0",
    "unified": "^10.1.2",
    "unist-util-modify-children": "^3.1.1",
    "unist-util-visit": "^4.1.2"
  },
  "peerDependencies": {
    "@storybook/addon-actions": ">=6.5.16",
    "@storybook/csf": "0.0.2--canary.4566f4d.1",
    "@storybook/react": "^6.5.16",
    "gatsby": ">=5.7.0",
    "gatsby-plugin-image": ">=3.7.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "yup": ">=1.0.2"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "workspace:*",
    "@amazeelabs/prettier-config": "workspace:*",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-multi-entry": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@storybook/csf": "0.0.2--canary.4566f4d.1",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/hast": "2.3.4",
    "@types/lodash": "4.14.191",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/unist": "2.0.6",
    "@vitejs/plugin-react": "3.1.0",
    "esbuild": "0.17.12",
    "eslint": "8.36.0",
    "gatsby": "5.7.0",
    "gatsby-plugin-image": "3.7.0",
    "jsdom": "21.1.1",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.19.1",
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-multi-input": "1.3.3",
    "typescript": "4.9.5",
    "vite": "4.2.0",
    "vite-plugin-dts": "1.7.3",
    "vitest": "0.29.3",
    "zustand": "4.3.6"
  },
  "scripts": {
    "build": "pnpm build:index && pnpm build:gatsby && pnpm build:storybook && pnpm build:react-intl && pnpm build:formik",
    "build:index": "ENTRYPOINT=index vite build",
    "build:gatsby": "ENTRYPOINT=gatsby vite build",
    "build:storybook": "ENTRYPOINT=storybook vite build",
    "build:formik": "ENTRYPOINT=formik vite build",
    "build:react-intl": "ENTRYPOINT=react-intl vite build",
    "dev": "vite build --watch",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "access": "public"
  }
}
