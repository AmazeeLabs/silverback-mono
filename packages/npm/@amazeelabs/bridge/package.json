  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.42",
    "@amazeelabs/prettier-config": "1.1.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.42"
  },
  "keywords": [],
  "author": "AmazeeLabs <development@amazeelabs.com>"
