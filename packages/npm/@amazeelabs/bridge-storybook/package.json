{
  "name": "@amazeelabs/bridge-storybook",
  "version": "1.1.0",
  "description": "Bridge implementation for storybook.",
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf build && swc ./src -d ./build && tsc --emitDeclarationOnly",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix"
  },
  "optionalDependencies": {
    "@types/react": "^18.2.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@storybook/addon-actions": ">=7",
    "react": ">=18"
  },
  "devDependencies": {
    "@amazeelabs/bridge": "workspace:*",
    "@amazeelabs/eslint-config": "1.4.43",
    "@amazeelabs/prettier-config": "1.1.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.55"
  },
  "keywords": [],
  "author": "AmazeeLabs <development@amazeelabs.com>"
}
