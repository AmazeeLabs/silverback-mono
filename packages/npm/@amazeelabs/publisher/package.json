{
  "name": "@amazeelabs/publisher",
  "version": "2.4.4",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/exports.cjs",
  "types": "./dist/exports.d.ts",
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "pnpm build --watch",
    "publisher": "node ./dist/cli.js",
    "start": "nodemon ./dist/cli.js",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run",
    "test:integration": "exit 0"
  },
  "dependencies": {
    "@amazeelabs/publisher-elements": "workspace:*",
    "@amazeelabs/publisher-shared": "workspace:*",
    "@amazeelabs/publisher-ui": "workspace:*",
    "@slack/webhook": "^6.1.0",
    "cookie-parser": "1.4.6",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-session": "1.17.3",
    "express-ws": "5.0.2",
    "fs-extra": "^11.1.1",
    "http-proxy-middleware": "2.0.6",
    "http-terminator": "3.2.0",
    "memorystore": "1.6.7",
    "node-fetch": "^3.3.1",
    "referrer-policy": "^1.2.0",
    "rxjs": "7.8.1",
    "sequelize": "^6.32.0",
    "simple-oauth2": "5.0.0",
    "sqlite3": "^5.1.6",
    "strip-ansi": "7.1.0",
    "terminate": "2.6.1",
    "ts-import": "4.0.0-beta.10",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.43",
    "@amazeelabs/prettier-config": "1.1.3",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/express-ws": "3.0.1",
    "@types/fs-extra": "11.0.1",
    "@types/node": "16.18.36",
    "@types/simple-oauth2": "5.0.4",
    "esbuild": "0.18.2",
    "eslint": "8.42.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "rollup": "3.25.1",
    "rollup-plugin-dts": "6.0.2",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "typescript": "5.1.3",
    "vitest": "0.32.0"
  },
  "engines": {
    "node": ">=16"
  }
}
