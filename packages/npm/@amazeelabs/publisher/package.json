    "@slack/webhook": "^6.1.0",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-ws": "5.0.2",
    "http-proxy-middleware": "2.0.6",
    "http-terminator": "3.2.0",
    "referrer-policy": "^1.2.0",
    "rxjs": "7.8.0",
    "sequelize": "^6.28.0",
    "sqlite3": "^5.1.4",
