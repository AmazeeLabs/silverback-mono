{
  "name": "@amazeelabs/publisher",
  "version": "0.6.0",
  "license": "MIT",
  "bin": "./cli.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "develop": "concurrently \"yarn build:server:watch\" \"yarn dev:server\" \"yarn dev:ui\"",
    "dev:ui": "vite",
    "dev:server": "cd test && nodemon --watch ../build ../build/server.js",
    "build:ui": "vite build --base=/___status/",
    "build:server": "rollup -c rollup.config.js",
    "build:server:watch": "rollup -c rollup.config.js -w",
    "build:tailwind:watch": "tailwind -w -i tailwind.css -o tailwind.build.css",
    "build:tailwind": "tailwind -i tailwind.css -o tailwind.build.css",
    "build:watch": "concurrently 'yarn build:server:watch' 'yarn build:tailwind:watch' 'yarn storybook'",
    "start": "cd test && ../cli.js",
    "preview": "vite preview",
    "prepare": "yarn build:tailwind && yarn build:ui && yarn build:server",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "postinstall": "prisma generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express-basic-auth": "^1.2.1",
    "prisma": "3.12.0",
    "referrer-policy": "^1.2.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.25",
    "@amazeelabs/jest-preset": "^1.3.28",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@babel/core": "7.17.9",
    "@react-rxjs/core": "0.9.5",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "21.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.2.0",
    "@rollup/plugin-replace": "4.0.0",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/web-components": "6.4.22",
    "@types/colors": "1.2.1",
    "@types/express": "4.17.13",
    "@types/express-ws": "3.0.1",
    "@types/http-proxy-middleware": "1.0.0",
    "@types/jest": "27.4.1",
    "@types/lodash-es": "4.17.6",
    "@types/morgan": "1.9.3",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/react-lazylog": "4.5.1",
    "@vitejs/plugin-react": "1.3.1",
    "babel-loader": "8.2.4",
    "clsx": "^1.1.1",
    "colors": "1.4.0",
    "concurrently": "7.1.0",
    "cosmiconfig": "7.0.1",
    "esbuild": "0.14.36",
    "eslint": "8.13.0",
    "eslint-plugin-storybook": "0.5.8",
    "express": "4.17.3",
    "express-ws": "5.0.2",
    "http-proxy-middleware": "2.0.4",
    "http-terminator": "3.2.0",
    "jest": "27.5.1",
    "lit": "2.2.2",
    "lodash-es": "4.17.21",
    "morgan": "1.10.0",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-lazylog": "4.5.3",
    "react-rxjs": "3.0.0-alpha.6",
    "rollup": "2.70.1",
    "rollup-plugin-esbuild": "4.9.1",
    "rollup-plugin-lit-css": "3.2.1",
    "rxjs": "7.5.5",
    "rxjs-shell": "3.1.2",
    "serve": "13.0.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3",
    "vite": "2.9.4"
  }
}
