{
  "name": "@amazeelabs/publisher",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "develop": "concurrently \"yarn build:server:watch\" \"yarn dev:server\" \"yarn dev:ui\"",
    "dev:ui": "vite",
    "dev:server": "nodemon --watch build ./build/server.js",
    "build:ui": "vite build --base=/___status/",
    "build:server": "rollup -c rollup.config.js",
    "build:server:watch": "rollup -c rollup.config.js -w",
    "start": "node build/server.js",
    "preview": "vite preview",
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "yarn test:static && yarn test:unit && yarn test:integration"
  },
  "dependencies": {
    "cosmiconfig": "^7.0.1",
    "express": "^4.17.2",
    "express-ws": "^5.0.2",
    "http-proxy-middleware": "^2.0.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "prisma": "^3.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazylog": "^4.5.3",
    "react-query": "^3.34.14",
    "react-use-websocket": "^3.0.0",
    "rxjs": "^7.5.2",
    "rxjs-shell": "^3.1.2"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.12",
    "@amazeelabs/jest-preset": "^1.3.16",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@prisma/client": "^3.8.1",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jest": "^27",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-lazylog": "^4.5.1",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.14",
    "eslint": "^8",
    "jest": "^27",
    "nodemon": "^2.0.15",
    "prettier": "^2",
    "rollup": "^2.66.1",
    "rollup-plugin-esbuild": "^4.8.2",
    "serve": "^13.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.2"
  }
}
