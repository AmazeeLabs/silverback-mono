{
  "name": "@amazeelabs/publisher",
  "version": "0.14.10",
  "license": "MIT",
  "bin": "./cli.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "develop": "concurrently \"pnpm build:server:watch\" \"pnpm dev:server\" \"pnpm dev:ui\"",
    "dev:ui": "vite",
    "dev:server": "cd test && nodemon --watch ../build ../build/server.js",
    "build:ui": "vite build --base=/___status/",
    "build:server": "rollup -c rollup.config.ts --configPlugin typescript",
    "build:server:watch": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "build:tailwind:watch": "tailwind -w -i tailwind.css -o tailwind.build.css",
    "build:tailwind": "tailwind -i tailwind.css -o tailwind.build.css",
    "build:watch": "concurrently 'pnpm build:server:watch' 'pnpm build:tailwind:watch' 'pnpm storybook'",
    "start": "cd test && ../cli.js",
    "preview": "vite preview",
    "build": "pnpm build:tailwind && pnpm build:ui && pnpm build:server",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@slack/webhook": "^6.1.0",
    "body-parser": "^1.20.1",
    "bytes": "^3.1.2",
    "cors": "^2.8.5",
    "express-basic-auth": "^1.2.1",
    "lodash": "^4.17.21",
    "mariadb": "^3.0.2",
    "mysql2": "^2.3.3",
    "referrer-policy": "^1.2.0",
    "sequelize": "^6.28.0",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.40",
    "@amazeelabs/jest-preset": "^1.3.44",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@amazeelabs/react-framework-bridge": "workspace:^2.3.14",
    "@babel/core": "7.20.12",
    "@headlessui/react": "1.7.8",
    "@react-rxjs/core": "0.10.3",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-commonjs": "23.0.7",
    "@rollup/plugin-json": "5.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.2",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-toolbars": "6.5.16",
    "@storybook/addon-viewport": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "^6.5.15",
    "@storybook/web-components": "6.5.16",
    "@types/body-parser": "^1.19.2",
    "@types/colors": "1.2.1",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.16",
    "@types/express-ws": "3.0.1",
    "@types/http-proxy-middleware": "1.0.0",
    "@types/jest": "29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "4.17.6",
    "@types/morgan": "1.9.4",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-lazylog": "4.5.1",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "9.1.2",
    "clsx": "1.2.1",
    "colors": "1.4.0",
    "concurrently": "7.6.0",
    "cosmiconfig": "8.0.0",
    "esbuild": "0.17.5",
    "eslint": "8.33.0",
    "eslint-plugin-storybook": "0.6.10",
    "express": "4.18.2",
    "express-ws": "5.0.2",
    "http-proxy-middleware": "2.0.6",
    "http-terminator": "3.2.0",
    "jest": "29.4.1",
