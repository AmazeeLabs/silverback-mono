{
  "name": "@amazeelabs/publisher",
  "version": "2.1.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/exports.cjs",
  "types": "./dist/exports.d.ts",
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "pnpm build --watch",
    "publisher": "node ./dist/cli.js",
    "start": "nodemon ./dist/cli.js",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "vitest run",
    "test:integration": "exit 0"
  },
  "dependencies": {
    "@amazeelabs/publisher-elements": "workspace:^2.0.5",
    "@amazeelabs/publisher-shared": "workspace:^2.0.4",
    "@amazeelabs/publisher-ui": "workspace:^2.0.5",
    "@slack/webhook": "^6.1.0",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-ws": "5.0.2",
    "http-proxy-middleware": "2.0.6",
    "http-terminator": "3.2.0",
    "referrer-policy": "^1.2.0",
    "rxjs": "7.8.0",
    "sequelize": "^6.29.3",
    "sqlite3": "^5.1.6",
    "strip-ansi": "6.0.1",
    "ts-import": "4.0.0-beta.10",
    "zustand": "4.3.6",
    "zx": "^7.2.1"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.42",
    "@amazeelabs/jest-preset": "^1.3.45",
    "@amazeelabs/prettier-config": "^1.1.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-ws": "3.0.1",
    "@types/jest": "29.5.0",
    "@types/node": "16.18.16",
    "esbuild": "0.17.12",
    "eslint": "8.36.0",
    "nodemon": "2.0.21",
    "prettier": "2.8.4",
    "rollup": "3.19.1",
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "typescript": "4.9.5",
    "vitest": "0.29.3"
  },
  "engines": {
    "node": ">=16"
  }
}
