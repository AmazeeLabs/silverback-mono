{
  "name": "@amazeelabs/publisher",
  "version": "0.2.1",
  "license": "MIT",
  "bin": "./cli.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "develop": "concurrently \"yarn build:server:watch\" \"yarn dev:server\" \"yarn dev:ui\"",
    "dev:ui": "vite",
    "dev:server": "cd test && nodemon --watch ../build ../build/server.js",
    "build:ui": "vite build --base=/___status/",
    "build:server": "rollup -c rollup.config.js",
    "build:server:watch": "rollup -c rollup.config.js -w",
    "start": "cd test && ../cli.js",
    "preview": "vite preview",
    "prepare": "yarn build:ui && yarn build:server",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "test:static && test:unit && test:integration",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "3.9.2",
    "prisma": "3.9.2"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.17",
    "@amazeelabs/jest-preset": "^1.3.20",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@react-rxjs/core": "0.8.4",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@types/colors": "1.2.1",
    "@types/express": "4.17.13",
    "@types/express-ws": "3.0.1",
    "@types/http-proxy-middleware": "1.0.0",
    "@types/jest": "27.4.0",
    "@types/morgan": "1.9.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-lazylog": "4.5.1",
    "@vitejs/plugin-react": "1.2.0",
    "colors": "1.4.0",
    "concurrently": "7.0.0",
    "cosmiconfig": "7.0.1",
    "esbuild": "0.14.22",
    "eslint": "8.9.0",
    "express": "4.17.3",
    "express-ws": "5.0.2",
    "http-proxy-middleware": "2.0.3",
    "http-terminator": "3.0.4",
    "jest": "27.5.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-lazylog": "4.5.3",
    "react-rxjs": "3.0.0-alpha.6",
    "rollup": "2.67.2",
    "rollup-plugin-esbuild": "4.8.2",
    "rxjs": "7.5.4",
    "rxjs-shell": "3.1.2",
    "serve": "13.0.2",
    "typescript": "4.5.5",
    "vite": "2.8.3"
  }
}
