{
  "name": "@amazeelabs/publisher",
  "version": "0.14.1",
  "license": "MIT",
  "bin": "./cli.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "develop": "concurrently \"yarn build:server:watch\" \"yarn dev:server\" \"yarn dev:ui\"",
    "dev:ui": "vite",
    "dev:server": "cd test && nodemon --watch ../build ../build/server.js",
    "build:ui": "vite build --base=/___status/",
    "build:server": "rollup -c rollup.config.js",
    "build:server:watch": "rollup -c rollup.config.js -w",
    "build:tailwind:watch": "tailwind -w -i tailwind.css -o tailwind.build.css",
    "build:tailwind": "tailwind -i tailwind.css -o tailwind.build.css",
    "build:watch": "concurrently 'yarn build:server:watch' 'yarn build:tailwind:watch' 'yarn storybook'",
    "start": "cd test && ../cli.js",
    "preview": "vite preview",
    "prepare": "yarn build:tailwind && yarn build:ui && yarn build:server",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@slack/webhook": "^6.1.0",
    "body-parser": "^1.20.1",
    "bytes": "^3.1.2",
    "cors": "^2.8.5",
    "express-basic-auth": "^1.2.1",
    "mariadb": "^3.0.2",
    "mysql2": "^2.3.3",
    "referrer-policy": "^1.2.0",
    "sequelize": "^6.25.3",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.3",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-toolbars": "^6.5.13",
    "@storybook/addon-viewport": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/web-components": "^6.5.13",
    "@types/cors": "^2.8.12",
    "@amazeelabs/eslint-config": "^1.4.36",
    "@amazeelabs/jest-preset": "^1.3.40",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@babel/core": "7.19.6",
    "@react-rxjs/core": "0.10.3",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@rollup/plugin-replace": "4.0.0",
    "@types/colors": "1.2.1",
    "@types/express": "4.17.14",
    "@types/express-ws": "3.0.1",
    "@types/http-proxy-middleware": "1.0.0",
    "@types/jest": "29.2.1",
    "@types/lodash-es": "4.17.6",
    "@types/morgan": "1.9.3",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-lazylog": "4.5.1",
    "@vitejs/plugin-react": "2.2.0",
    "babel-loader": "8.2.5",
    "clsx": "1.2.1",
    "colors": "1.4.0",
    "concurrently": "7.5.0",
    "cosmiconfig": "7.0.1",
    "esbuild": "0.15.13",
    "eslint": "8.26.0",
    "eslint-plugin-storybook": "0.6.7",
    "express": "4.18.2",
    "express-ws": "5.0.2",
    "http-proxy-middleware": "2.0.6",
    "http-terminator": "3.2.0",
    "jest": "29.2.2",
    "lit": "2.4.0",
    "lodash-es": "4.17.21",
    "morgan": "1.10.0",
    "nodemon": "2.0.20",
