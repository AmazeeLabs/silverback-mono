  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@amazeelabs/eslint-config": "^1.4.30",
    "@amazeelabs/jest-preset": "^1.3.33",
    "@amazeelabs/prettier-config": "^1.1.1",
    "iframe-resizer-react": "^1.1.0"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.4",
    "@amazeelabs/jest-preset": "^1.3.10",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "28.1.2",
    "eslint": "8.18.0",
    "jest": "28.1.1",
    "lint-staged": "13.0.2",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "prepare": "tsc",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
