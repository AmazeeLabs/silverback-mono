{
  "name": "@amazeelabs/gatsby-fragments",
  "version": "1.1.2",
  "description": "Generates fragment.ts files from .gql files to append vendor prefix.",
  "author": "Amazee Labs",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "bin": "./cli.js",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.28",
    "@amazeelabs/jest-preset": "^1.3.34",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.2",
    "commander": "9.3.0",
    "eslint": "8.18.0",
    "glob": "8.0.3",
    "jest": "28.1.1",
    "mock-fs": "5.1.2",
    "prettier": "2.7.1",
    "typescript": "4.6.4"
  },
  "scripts": {
    "prepare": "rollup -c rollup.config.js",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "yarn test:static && yarn test:unit && yarn test:integration"
  },
  "volta": {
    "node": "16.15.1"
  }
}
