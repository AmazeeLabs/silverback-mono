{
  "name": "@amazeelabs/gatsby-fragments",
  "version": "1.1.29",
  "description": "Generates fragment.ts files from .gql files to append vendor prefix.",
  "author": "Amazee Labs",
  "main": "index.cjs",
  "type": "commonjs",
  "license": "MIT",
  "private": false,
  "bin": "./cli.cjs",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "1.4.42",
    "@amazeelabs/jest-preset": "1.3.46",
    "@amazeelabs/prettier-config": "1.1.2",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.0",
    "@types/mock-fs": "4.13.1",
    "commander": "10.0.0",
    "eslint": "8.36.0",
    "glob": "8.1.0",
    "jest": "29.5.0",
    "mock-fs": "5.2.0",
    "prettier": "2.8.4",
    "rollup": "3.19.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "volta": {
    "node": "18.15.0"
