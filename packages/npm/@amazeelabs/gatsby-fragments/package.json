{
  "name": "@amazeelabs/gatsby-fragments",
  "version": "1.1.17",
  "description": "Generates fragment.ts files from .gql files to append vendor prefix.",
  "author": "Amazee Labs",
  "main": "index.cjs",
  "type": "commonjs",
  "license": "MIT",
  "private": false,
  "bin": "./cli.cjs",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.36",
    "@amazeelabs/jest-preset": "^1.3.40",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.1",
    "commander": "9.4.1",
    "eslint": "8.26.0",
    "glob": "8.0.3",
    "jest": "29.2.2",
    "mock-fs": "5.2.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "scripts": {
    "prepare": "rollup -c rollup.config.mjs",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "yarn test:static && yarn test:unit && yarn test:integration"
  },
  "volta": {
    "node": "18.12.0"
  }
}
