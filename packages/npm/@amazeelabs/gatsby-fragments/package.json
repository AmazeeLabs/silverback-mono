{
  "name": "@amazeelabs/gatsby-fragments",
  "version": "1.1.21",
  "description": "Generates fragment.ts files from .gql files to append vendor prefix.",
  "author": "Amazee Labs",
  "main": "index.cjs",
  "type": "commonjs",
  "license": "MIT",
  "private": false,
  "bin": "./cli.cjs",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.38",
    "@amazeelabs/jest-preset": "^1.3.41",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@rollup/plugin-commonjs": "23.0.7",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "10.0.1",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.5",
    "commander": "9.5.0",
    "eslint": "8.31.0",
    "glob": "8.0.3",
    "jest": "29.3.1",
    "mock-fs": "5.2.0",
    "prettier": "2.8.2",
    "typescript": "4.9.4"
  },
  "scripts": {
    "prepare": "rollup -c rollup.config.mjs",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "exit 0",
    "test:watch": "jest --watch",
    "test": "yarn test:static && yarn test:unit && yarn test:integration"
  },
  "volta": {
    "node": "18.13.0"
  }
}
