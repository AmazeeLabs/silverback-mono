{
  "name": "@-drupal/gatsby_build_monitor",
  "version": "1.4.57",
  "description": "Notifies about the Gatsby Build Status via the Toolbar.",
  "publishConfig": {
    "access": "public",
    "registry": "http://localhost:4873",
    "repository": "git@git.drupal.org:project/gatsby_build_monitor.git",
    "branch": "main"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.0",
    "eslint": "8.36.0",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.6",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@amazeelabs/eslint-config": "workspace:*",
    "@amazeelabs/jest-preset": "workspace:*",
    "@amazeelabs/prettier-config": "workspace:*",
    "@amazeelabs/sync-composer-version": "workspace:*"
  },
  "scripts": {
    "precommit": "lint-staged",
    "watch": "jest --watch",
    "version": "sync-composer-version",
    "test:static": "eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  }
