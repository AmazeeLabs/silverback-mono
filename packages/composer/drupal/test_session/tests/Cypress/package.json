{
  "name": "@-test_session/cypress-tests",
  "version": "1.1.63",
  "private": true,
  "scripts": {
    "watch": "jest --watch",
    "cypress:prepare": "./scripts/setup.sh && ./scripts/start.sh 'true'",
    "cypress:run": "pnpm cypress:prepare && pnpm cypress run",
    "cypress:open": "pnpm cypress:prepare && pnpm cypress open",
    "precommit": "lint-staged",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "pnpm cypress:run",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@-drupal/test_session": "^1.2.52",
    "@amazeelabs/eslint-config": "^1.4.39",
    "@amazeelabs/jest-preset": "^1.3.43",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "29.4.0",
    "@types/node": "16.18.11",
    "cypress": "9.7.0",
    "eslint": "8.33.0",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "typescript": "4.9.5"
  }
}
