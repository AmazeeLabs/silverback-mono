{
  "name": "drupal/cypress",
  "type": "drupal-module",
  "description": "Cypress E2E testing helpers",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "extra": {
    "patches": {
      "drupal/core": {
        "Language negotiation weight": "https://www.drupal.org/files/issues/2018-10-17/3007386-language-negotiator-weight-2.patch"
      }
    },
    "drush": {
      "services": {
        "drush.services.yml": "^10"
      }
    }
  },
  "require": {
    "ext-json": "*",
    "alchemy/zippy": "dev-master",
    "drupal/test_session": "^1.0.0",
    "drush/drush": "^10.3.5"
  },
  "require-dev": {
    "drupal/cypress-dev": "*"
  }
}
