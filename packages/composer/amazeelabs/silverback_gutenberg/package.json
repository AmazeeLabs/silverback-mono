{
  "name": "@-amazeelabs/silverback_gutenberg",
  "version": "1.6.8",
  "author": "Amazee Labs <development@amazeelabs.com>",
  "publishConfig": {
    "access": "public",
    "registry": "http://localhost:4873",
    "repository": "git@github.com:AmazeeLabs/silverback_gutenberg.git",
    "branch": "main"
  },
  "scripts": {
