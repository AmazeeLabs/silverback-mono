{
  "name": "@-amazeelabs/silverback-persisted-queries",
  "version": "1.1.87",
  "description": "e2e tests for Silverback persisted queries.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback-gastby": "^1.2.151",
    "@amazeelabs/silverback-graphql-persisted": "^1.1.6"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.33",
    "@amazeelabs/jest-preset": "^1.3.37",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "28.1.8",
    "eslint": "8.23.0",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.3"
  },
  "scripts": {
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi && sp-test",
    "test:watch": "jest --watch"
  }
}
