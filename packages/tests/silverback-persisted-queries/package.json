{
  "name": "@-amazeelabs/silverback-persisted-queries",
  "version": "1.1.120",
  "description": "e2e tests for Silverback persisted queries.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback-gastby": "^2.0.0",
    "@amazeelabs/silverback-graphql-persisted": "^1.2.2"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.38",
    "@amazeelabs/jest-preset": "^1.3.41",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "29.2.5",
    "eslint": "8.31.0",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  },
  "scripts": {
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi && sp-test",
    "test:watch": "jest --watch"
  }
}
