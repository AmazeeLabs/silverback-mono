  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.15.15",
    "@-amazeelabs/silverback_gutenberg": "^1.6.11",
    "@amazeelabs/silverback-playwright": "^1.6.46"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.38",
