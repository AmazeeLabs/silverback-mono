{
  "name": "@-amazeelabs/silverback-gutenberg-tests",
  "version": "1.1.52",
  "description": "e2e tests for Silverback Gutenberg.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.13.16",
    "@-amazeelabs/silverback_gutenberg": "^1.4.1",
    "@amazeelabs/silverback-playwright": "^1.6.19"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.18",
    "@amazeelabs/jest-preset": "^1.3.21",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.4.1",
    "eslint": "8.9.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
