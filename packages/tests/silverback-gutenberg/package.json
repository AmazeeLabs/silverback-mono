    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "29.2.5",
    "eslint": "8.31.0",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
