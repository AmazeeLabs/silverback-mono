{
  "name": "@-amazeelabs/silverback-gutenberg-tests",
  "version": "1.1.121",
  "description": "e2e tests for Silverback Gutenberg.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.14.28",
    "@-amazeelabs/silverback_gutenberg": "^1.6.4",
    "@amazeelabs/silverback-playwright": "^1.6.39"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.33",
    "@amazeelabs/jest-preset": "^1.3.37",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "28.1.8",
    "eslint": "8.23.0",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "typescript": "4.8.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "prepare": "exit 0",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "sp-test",
    "test:watch": "jest --watch"
