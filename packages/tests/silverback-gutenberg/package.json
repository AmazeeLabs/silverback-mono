{
  "name": "@-amazeelabs/silverback-gutenberg-tests",
  "version": "1.1.98",
  "description": "e2e tests for Silverback Gutenberg.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback-drupal": "^1.14.6",
    "@-amazeelabs/silverback_gutenberg": "^1.5.13",
    "@amazeelabs/silverback-playwright": "^1.6.32"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.30",
    "@amazeelabs/jest-preset": "^1.3.33",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.5.2",
    "eslint": "8.18.0",
    "jest": "27.5.1",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "ts-jest": "27.1.5",
    "typescript": "4.7.4"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "prepare": "exit 0",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "sp-test",
    "test:watch": "jest --watch"
  }
