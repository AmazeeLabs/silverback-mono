{
  "name": "@-amazeelabs/silverback-gatsby-tests",
  "version": "1.1.28",
  "description": "e2e tests for Drupal-Gatsby connection.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback_gatsby": "^1.16.0",
    "@amazeelabs/gatsby-source-silverback": "^1.5.12",
    "@amazeelabs/silverback-playwright": "^1.6.16"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.15",
    "@amazeelabs/jest-preset": "^1.3.18",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.4.0",
    "eslint": "8.8.0",
    "jest": "27.4.7",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "scripts": {
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi && sp-test",
    "test:watch": "jest --watch"
  }
}
