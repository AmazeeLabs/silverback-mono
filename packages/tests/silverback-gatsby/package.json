{
  "name": "@-amazeelabs/silverback-gatsby-tests",
  "version": "1.1.84",
  "description": "e2e tests for Drupal-Gatsby connection.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback_gatsby": "^1.25.1",
    "@amazeelabs/gatsby-source-silverback": "^1.9.2",
    "@amazeelabs/silverback-playwright": "^1.6.38"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.32",
    "@amazeelabs/jest-preset": "^1.3.36",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "28.1.8",
    "eslint": "8.23.0",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.2"
  },
  "scripts": {
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi && sp-test",
    "test:watch": "jest --watch"
  }
}
