{
  "name": "@-amazeelabs/silverback-gatsby-tests",
  "version": "1.1.34",
  "description": "e2e tests for Drupal-Gatsby connection.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback_gatsby": "^1.16.2",
    "@amazeelabs/gatsby-source-silverback": "^1.6.0",
    "@amazeelabs/silverback-playwright": "^1.6.20"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.19",
    "@amazeelabs/jest-preset": "^1.3.22",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "27.4.1",
    "eslint": "8.10.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "typescript": "4.6.2"
  },
  "scripts": {
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi && sp-test",
    "test:watch": "jest --watch"
  }
}
