{
  "name": "@-amazeelabs/silverback-gatsby-tests",
  "version": "1.1.97",
  "description": "e2e tests for Drupal-Gatsby connection.",
  "author": "Amazee Labs",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@-amazeelabs/silverback_gatsby": "^1.28.0",
    "@amazeelabs/gatsby-source-silverback": "^1.9.9",
    "@amazeelabs/silverback-playwright": "^1.6.44"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.36",
    "@amazeelabs/jest-preset": "^1.3.40",
    "@amazeelabs/prettier-config": "^1.1.1",
    "@types/jest": "29.2.4",
    "eslint": "8.30.0",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  },
  "scripts": {
    "prepare": "exit 0",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi && sp-test",
    "test:watch": "jest --watch"
  }
}
